fn add(a: i32, b: i32) -> i32 {
    return a + b;
}

fn multiply(a: i32, b: i32) -> i32 {
    return a * b;
}

fn main() -> i32 {
    let x: i32 = 5;
    let y: i32 = 3;
    
    let sum: i32 = add(x, y);
    let product: i32 = multiply(x, y);
    
    print(sum);
    print(product);
    
    return 0;
}